@model App.Web.Core.Models.Application.ApplicationModel

@{
    ViewData["Title"] = Model.Name;
    Layout = "~/Views/Shared/_layoutAdmin.cshtml";
}

<div class="admin-container">
    <div class="row">
        <div class="col-3">
            @Html.Partial("_applicationNavigation", Model.Id, new ViewDataDictionary(ViewData){})
        </div>
        <div class="col">
            <h2 class="admin-heading d-flex justify-content-between align-items-center text-muted">
                <span>@Model.Name</span>
            </h2>
        </div>
    </div>
</div>


